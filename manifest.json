{
  "manifest_version": 3,
  "name": "Task Helper",
  "author": "Dennis Liu",
  "version": "1.4.0",
  "description": "A Chrome extension to help you quickly navigate to Jira tickets",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "icons": {
    "16": "images/data16.png",
    "32": "images/data32.png",
    "64": "images/data64.png",
    "128": "images/data128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/data16.png",
      "32": "images/data32.png",
      "64": "images/data64.png",
      "128": "images/data128.png"
    }
  }
} 